<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Presentation" parallel="tests" thread-count="3">


    <test name="SanityTestSuite">
        <groups>
            <run>
                <include name="sanity"/>
            </run>
        </groups>

        <classes>
            <class name="VeterinariansTests"></class>
        </classes>
    </test>

    <test name="HomePageAndOwnersPagesTestSuite">
        <classes>
            <class name="OwnersPageTests"></class>
            <class name="HomepageTests"></class>
        </classes>
    </test>

</suite>

